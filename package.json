{
  "name": "sii-api",
  "version": "1.0.0",
  "description": "Scrapper para generar una api del sii",
  "main": "src/main.js",
  "scripts": {
    "start": "pm2-runtime src/main.js --watch --raw",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "husky install",
    "pre-commit": "lint-staged",
    "create-doc": "openapi-combine merge -i src/docs/spec/spec.json -o src/docs/openapi.json -t json"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "author": "Alfredo Monroy (Freddy)",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "axios": "^0.21.4",
    "cheerio": "^1.0.0-rc.10",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.1.1",
    "pm2": "^5.1.1",
    "qs": "^6.10.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-watch": "^7.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.6",
    "node-env-run": "^4.0.2",
    "openapi-combine": "^0.3.1",
    "prettier": "^2.4.1",
    "stylelint": "^13.13.1"
  }
}
